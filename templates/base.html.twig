<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Matuka{% endblock %}</title>
        
    </head>
{% block body%}
        <body>
            <div class="container">
{% block stylesheets %}
    <style type="text/css">
    .header {
    background: #8a8a8a;
    }
    .header .columns {
    padding-bottom: 0;
    }
    .header p {
    color: #fff;
    margin-bottom: 0;
    }
    .header .wrapper-inner {
    padding: 20px;
    }
    .header .container {
    background: #8a8a8a;
    }
    .wrapper.secondary {
    background: #f3f3f3;
    }

    h1
    {
        margin-top: 100px;
        text-align: center; 
    }

    #divboard
    {
        text-align: center;
        margin-top: 250px;
    }

    #roues {
    display: none;
    }

    #roulements {
    display: none;
    }

    #trucks {
    display: none;
    }

    #trucks img + img {
    margin-left: 120px;
    }

    #roulements img + img {
    margin-left: 120px;
    }

    #panier {
    display: none;
    }

    </style>
{% endblock %}
    <container>

    <spacer size="16"></spacer>

    <row>
        <div id="divboard">

            <div id="skates">

                <img class="skates" onclick="changeSkateImage(this)" onclick="javascript:changeSize('target',500);" src="https://images.blue-tomato.com/is/image/bluetomato/303492209_front.jpg-7qsnaug8bWKM5EHfEVbVM27zMbM/Lord+Nermal+Board+8+25+Skate+Deck.jpg?$b8$" width="220" height="290">
                <img class="skates" onclick="changeSkateImage(this)" src="https://images-na.ssl-images-amazon.com/images/I/51BLNp86ZFL._SL1340_.jpg" width="220" height="290">
                <img class="skates" onclick="changeSkateImage(this)" src="https://images-na.ssl-images-amazon.com/images/I/511o33ZcYML._SL1340_.jpg" width="220" height="290">
                <img class="skates" onclick="changeSkateImage(this)" src="http://www.tranzport.ch/wp-content/uploads/2018/01/JimmyCaoLBxSM80x32Deck.jpg" width="220" height="290">

            </div>

            <div id="trucks">
                <img class="trucks" onclick="changeTrucksImage(this)" src="data:image/jpg;base64," width="220" height="290">
                <img class="trucks" onclick="changeTrucksImage(this)" src="data:image/jpg;base64," width="220" height="290">
                <img class="trucks" onclick="changeTrucksImage(this)" src="data:image/jpg;base64," width="220" height="290">
                <img class="trucks" onclick="changeTrucksImage(this)" src="data:image/jpg;base64," width="220" height="290">
            </div>

            <div id="roulements">
                <img class="example" onclick="changeRoulementsImage(this)" src="https://cdn2.playskateshop.com/89-large/roulements-de-skate-bones-reds.jpg" width="220" height="290">
                <img class="example" onclick="changeRoulementsImage(this)" src="https://www.xtreme-skate.com/skate-shop/3256-large_default/minilogo-minilogo-abec-5-roulements-skate.jpg" width="220" height="290">
            </div>

            <div id="roues">
            
            <img class="example" onclick="afficherPanier(this)" src="https://fulkit-skateboards.com/3238-large_default/roues-skateboard-nudes-52mm-blanches.jpg" width="220" height="290">
            <img class="example" onclick="afficherPanier(this)" src="https://fulkit-skateboards.com/4715-large_default/powell-peralta-rat-bones-60mm-roues-skateboard.jpg" width="220" height="290">
            
            </div>

            <div id="panier">
            

            
            </div>
        </div>
        </columns>
    </row>
    
    </container></div>

            {% block javascripts %}
                <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
                <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
                <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
                <script>
                
                    let skateImgs = document.getElementsByClassName('skates');
let skateDiv = document.getElementById('skates');
let trucksDiv = document.getElementById('trucks');
let roulementsDiv = document.getElementById('roulements');
let rouesDiv = document.getElementById('roues');
let panierDiv = document.getElementById('panier');
let trucksImgs = document.getElementsByClassName('trucks');
let contenuePanier = [];

function changeSkateImage(element) {

    skateDiv.style.display = "none";
    trucksDiv.style.display = "block";
    contenuePanier[0] = element;
}

function changeTrucksImage(element) {
    trucksDiv.style.display = "none";
    roulementsDiv.style.display = "block";
    contenuePanier[1] = element;
}

function changeRoulementsImage(element) {
    roulementsDiv.style.display = "none";
    rouesDiv.style.display = "block"
    contenuePanier[2] = element;
}

function afficherPanier(element) {
    contenuePanier[3] = element;
    rouesDiv.style.display = "none";
    for(var i = 0; i < contenuePanier.length; i++) {
        panierDiv.appendChild(contenuePanier[i]);
    }
    for(var  i = 0; i < panierDiv.childElementCount; i = i + 1) {
        panierDiv.children[i].setAttribute('onclick', '');
    }
    panierDiv.style.display = "block";
}
                
                </script>
            {% endblock %}
    </body>
{% endblock %}
</html>
